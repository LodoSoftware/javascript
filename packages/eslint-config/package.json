{
  "name": "@d3banking/eslint-config",
  "version": "15.0.1",
  "description": "D3 Banking fork of Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused",
    "test": "yarn --silent lint && yarn --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "1.4.2",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-import-resolver-webpack": "0.11.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.5.0",
    "prettier": "1.16.4"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-preset-airbnb": "^3.2.0",
    "babel-tape-runner": "^3.0.0",
    "eslint": "5.14.1",
    "eslint-find-rules": "^3.3.1",
    "tape": "^4.10.1"
  },
  "peerDependencies": {
    "eslint": ">= 5"
  }
}
