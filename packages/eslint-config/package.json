{
  "name": "@d3banking/eslint-config",
  "version": "15.5.1",
  "description": "D3 Banking fork of Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused",
    "test": "yarn --silent lint && yarn --silent tests-only",
    "release": "standard-version -a",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "prettier": "1.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-preset-airbnb": "^3.2.1",
    "babel-tape-runner": "^3.0.0",
    "eslint": "5.16.0",
    "eslint-find-rules": "^3.3.1",
    "standard-version": "^5.0.2",
    "tape": "^4.10.2"
  },
  "peerDependencies": {
    "eslint": ">= 5.14.0"
  }
}
