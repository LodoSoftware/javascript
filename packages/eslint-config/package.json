{
  "name": "@d3banking/eslint-config",
  "version": "14.0.0-rc.3",
  "description": "D3 Banking fork of Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "pretest": "eslint-find-rules --unused",
    "test": "npm run --silent lint && npm run --silent tests-only"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LodoSoftware/javascript-style-guide"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "airbnb",
    "javascript",
    "styleguide",
    "d3banking"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LodoSoftware/javascript-style-guide/issues"
  },
  "homepage": "https://github.com/LodoSoftware/javascript-style-guide",
  "dependencies": {
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "prettier": "1.15.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "babel-preset-airbnb": "^3.0.1",
    "babel-tape-runner": "^3.0.0",
    "eslint": "5.9.0",
    "eslint-find-rules": "^3.3.1",
    "tape": "^4.9.1"
  },
  "peerDependencies": {
    "eslint": ">= 5"
  }
}
